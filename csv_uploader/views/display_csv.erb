<html>
  <head></head>
  <body>
    <h1>Displaying CSV</h1>
    <table style="border: 1px solid;">
      <thead>
        <% @csv.shift.tap do |header_row| %>
          <% header_row.each do |header| %>
            <th><%= header %></th>
          <% end %>
        <% end %>
      </thead>
      <tbody>
        <% @csv.each do |row| %>
          <tr>
            <% row.each do |column| %>
              <td style="border: 1px solid;">
                <%= column %>
              </td>
            <% end %>
          </tr>
        <% end %>
      </tbody>
    </table>
  </body>
</html>
